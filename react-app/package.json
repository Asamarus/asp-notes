{
  "name": "asp-notes",
  "private": true,
  "version": "1.0.0",
  "author": "Demax",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "set NODE_ENV=development && tsc && vite build --mode development",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "lint:stylelint": "stylelint \"src/**/*.css\" --cache",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "openapi": "openapi-typescript ./docs/openapi.json -o ./src/misc/openapi.d.ts",
    "test": "npm run typecheck && npm run lint && npm run vitest && npm run build",
    "tcm": "tcm -p \"src/**/*.module.css\"",
    "deploy": "node deploy.cjs"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@hello-pangea/dnd": "^16.6.0",
    "@mantine/charts": "^7.12.0",
    "@mantine/code-highlight": "^7.12.0",
    "@mantine/core": "^7.12.0",
    "@mantine/dates": "^7.12.0",
    "@mantine/hooks": "^7.12.0",
    "@mantine/notifications": "^7.12.0",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "js-base64": "^3.7.7",
    "klona": "^2.0.6",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "openapi-fetch": "^0.10.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.26.0",
    "rison": "^0.1.1",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/rison": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^24.1.1",
    "msw": "^2.3.5",
    "openapi-typescript": "^7.3.0",
    "postcss": "^8.4.41",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "stylelint": "^16.8.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "terser": "^5.31.5",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
