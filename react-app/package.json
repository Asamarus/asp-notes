{
  "name": "asp-notes",
  "private": true,
  "version": "1.0.0",
  "author": "Demax",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "set NODE_ENV=development && tsc && vite build --mode development",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "lint:stylelint": "stylelint \"src/**/*.css\" --cache",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "openapi": "openapi-typescript ./docs/openapi.json -o ./src/misc/openapi.d.ts",
    "test": "npm run typecheck && npm run lint && npm run vitest && npm run build",
    "tcm": "tcm -p \"src/**/*.module.css\"",
    "deploy": "node deploy.cjs"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@hello-pangea/dnd": "^16.6.0",
    "@mantine/charts": "^7.9.2",
    "@mantine/code-highlight": "^7.9.2",
    "@mantine/core": "^7.9.2",
    "@mantine/dates": "^7.9.2",
    "@mantine/hooks": "^7.9.2",
    "@mantine/notifications": "^7.9.2",
    "clsx": "^2.1.1",
    "js-base64": "^3.7.7",
    "klona": "^2.0.6",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "openapi-fetch": "^0.9.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1",
    "rison": "^0.1.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/rison": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^24.0.0",
    "msw": "^2.3.0",
    "openapi-typescript": "^7.0.0-next.8",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "terser": "^5.31.0",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
